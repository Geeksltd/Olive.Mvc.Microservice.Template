{
    "Authentication": {
        "Cookie": {
            "Domain": "app.geeks.ltd"
        }
    },

    "Microservice": {
        "Me": {
            "Url": "https://my.microservice.name.%DOMAIN%"
        },
        "Hub": {
            "Url": "https://hub.app.geeks.ltd"
        }
    },

    "Aws": {
        "Region": "eu-west-1",
        "Secrets": {
            "Id": "geeksms/globalsearch"
        }
    },

    "Automated.Tasks": {
        "Enabled": true
    },

    "Logging": {
        "EventBus": {
            "Source": "Tasks",
            "QueueUrl": "https://sqs.eu-west-1.amazonaws.com/669486994886/GeeksMS-Logs"
        },
        "IncludeScopes": true,
        "LogLevel": {
            "Default": "Error"
        }
    },

    "Blob": {
        "S3": {
            "Bucket": "ms.globalsearch",
            "TempBucket": "ms.globalsearch.temp"
        }
    }
}
